		}
		System.out.println();
		
	}
	private static int prod(int i) {
		// TODO Auto-generated method stub
		return 0;
	}
	public static int prod(int prod[]) {
		int i=0;
				for(i=1;i<prod.length;i++)
				{
					if(prod[i]!=i)
						return -1;
					
				}
		return prod[i];

}}
